{
  "annotations": {"list": []},
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "gridPos": {"h": 2, "w": 24, "x": 0, "y": 0},
      "id": 1,
      "options": {
        "content": "# Database Performance Dashboard\n\nMonitoring PostgreSQL databases (Backend & AI Services), Redis cache, and Elasticsearch search engine.",
        "mode": "markdown"
      },
      "type": "text"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "mappings": [
            {"options": {"0": {"color": "red", "text": "DOWN"}, "1": {"color": "green", "text": "UP"}}, "type": "value"}
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [{"color": "red", "value": null}, {"color": "green", "value": 1}]
          }
        }
      },
      "gridPos": {"h": 4, "w": 12, "x": 0, "y": 2},
      "id": 2,
      "options": {
        "orientation": "horizontal",
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {"expr": "pg_up{database=\"backend\"}", "legendFormat": "PostgreSQL Backend", "refId": "A"},
        {"expr": "pg_up{database=\"ai-services\"}", "legendFormat": "PostgreSQL AI", "refId": "B"},
        {"expr": "redis_up", "legendFormat": "Redis", "refId": "C"},
        {"expr": "up{service=\"elasticsearch\"}", "legendFormat": "Elasticsearch", "refId": "D"}
      ],
      "title": "Database Services Status",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Connections",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 2},
      "id": 3,
      "options": {
        "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom"}
      },
      "targets": [
        {
          "expr": "pg_stat_database_numbackends{database=\"backend\"}",
          "legendFormat": "Backend DB Active Connections",
          "refId": "A"
        },
        {
          "expr": "pg_settings_max_connections{database=\"backend\"}",
          "legendFormat": "Backend DB Max Connections",
          "refId": "B"
        },
        {
          "expr": "pg_stat_database_numbackends{database=\"ai-services\"}",
          "legendFormat": "AI DB Active Connections",
          "refId": "C"
        }
      ],
      "title": "PostgreSQL Connection Usage",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Queries/sec",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "reqps"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "id": 4,
      "options": {
        "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom"}
      },
      "targets": [
        {
          "expr": "rate(pg_stat_database_xact_commit{database=\"backend\"}[5m])",
          "legendFormat": "Backend Commits/sec",
          "refId": "A"
        },
        {
          "expr": "rate(pg_stat_database_xact_rollback{database=\"backend\"}[5m])",
          "legendFormat": "Backend Rollbacks/sec",
          "refId": "B"
        },
        {
          "expr": "rate(pg_stat_database_xact_commit{database=\"ai-services\"}[5m])",
          "legendFormat": "AI Commits/sec",
          "refId": "C"
        }
      ],
      "title": "PostgreSQL Transaction Rate",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Milliseconds",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "ms"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
      "id": 5,
      "options": {
        "legend": {"calcs": ["mean", "p95"], "displayMode": "table", "placement": "bottom"}
      },
      "targets": [
        {
          "expr": "pg_stat_activity_max_tx_duration{database=\"backend\"}",
          "legendFormat": "Backend Max Query Duration",
          "refId": "A"
        },
        {
          "expr": "pg_stat_activity_max_tx_duration{database=\"ai-services\"}",
          "legendFormat": "AI Max Query Duration",
          "refId": "B"
        }
      ],
      "title": "PostgreSQL Query Duration",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 60},
              {"color": "red", "value": 80}
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 14},
      "id": 6,
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "showThresholdMarkers": true},
      "targets": [
        {
          "expr": "(pg_stat_database_numbackends{database=\"backend\"} / pg_settings_max_connections{database=\"backend\"}) * 100",
          "refId": "A"
        }
      ],
      "title": "Backend DB Connection Usage %",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 85}
            ]
          },
          "unit": "decbytes"
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 14},
      "id": 7,
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "showThresholdMarkers": true},
      "targets": [
        {
          "expr": "pg_database_size_bytes{database=\"backend\"}",
          "refId": "A"
        }
      ],
      "title": "Backend Database Size",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 20},
      "id": 8,
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "showThresholdMarkers": true},
      "targets": [
        {
          "expr": "(redis_memory_used_bytes / redis_memory_max_bytes) * 100",
          "refId": "A"
        }
      ],
      "title": "Redis Memory Usage %",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 0.6},
              {"color": "green", "value": 0.8}
            ]
          },
          "unit": "percentunit"
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 20},
      "id": 9,
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "showThresholdMarkers": true},
      "targets": [
        {
          "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total)",
          "refId": "A"
        }
      ],
      "title": "Redis Cache Hit Rate",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Operations/sec",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "ops"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "id": 10,
      "options": {
        "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom"}
      },
      "targets": [
        {
          "expr": "rate(redis_commands_processed_total[5m])",
          "legendFormat": "Commands Processed",
          "refId": "A"
        },
        {
          "expr": "rate(redis_keyspace_hits_total[5m])",
          "legendFormat": "Cache Hits",
          "refId": "B"
        },
        {
          "expr": "rate(redis_keyspace_misses_total[5m])",
          "legendFormat": "Cache Misses",
          "refId": "C"
        }
      ],
      "title": "Redis Operations",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Count",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
      "id": 11,
      "options": {
        "legend": {"calcs": ["mean", "last"], "displayMode": "table", "placement": "bottom"}
      },
      "targets": [
        {
          "expr": "elasticsearch_indices_docs{index=~\"products|orders|users\"}",
          "legendFormat": "{{index}} documents",
          "refId": "A"
        }
      ],
      "title": "Elasticsearch Document Count",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "Queries/sec",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "reqps"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
      "id": 12,
      "options": {
        "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom"}
      },
      "targets": [
        {
          "expr": "rate(elasticsearch_indices_search_query_total[5m])",
          "legendFormat": "Search Queries",
          "refId": "A"
        },
        {
          "expr": "rate(elasticsearch_indices_indexing_index_total[5m])",
          "legendFormat": "Indexing Operations",
          "refId": "B"
        }
      ],
      "title": "Elasticsearch Operations",
      "type": "timeseries"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["e-commerce", "database", "postgres", "redis", "elasticsearch"],
  "time": {"from": "now-1h", "to": "now"},
